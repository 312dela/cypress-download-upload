{
  "name": "cypressdownloadupload",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress run --reporter mochawesome --reporter-options reportDir=cypress/reports/html,overwrite=false,html=true,json=true",
    "generate-report": "npx mochawesome-merge cypress/reports/html/*.json | npx mochawesome-report-generator --reportDir cypress/reports/html --inline",
    "test:withreport": "npm run test && npm run generate-report"
  },
  "author": "dela rosa",
  "license": "ISC",
  "dependencies": {
    "convert-excel-to-json": "^1.7.0",
    "exceljs": "^4.4.0"
  },
  "devDependencies": {
    "cypress": "^14.5.0",
    "cypress-mochawesome-reporter": "^3.8.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^6.2.0"
  }
}